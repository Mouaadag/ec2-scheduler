# Configuration pour my-webApp EC2 Scheduler

# Instance EC2 configuration
instance_type = "t2.micro"
region = "us-east-1"

# Schedule configuration (démarrage à 8h du lundi au vendredi)
schedule = "cron(0 8 ? * MON-FRI *)"
vpc_id = "vpc-12345678"
subnet_ids = ["subnet-12345678", "subnet-87654321"]
tags = {
  Name = "Scolio-EC2-Scheduler-Dev"
  Environment = "Development"
}